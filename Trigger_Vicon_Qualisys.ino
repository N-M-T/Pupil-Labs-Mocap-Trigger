void setup() {  // initialize the serial communication:  Serial.begin(500000);}void loop() {  int raw_input = analogRead(A0);    // convert analog reading to digital  float volts = raw_input * (5.0 / 1023.0);    // Serial.println(volts);  // for testing connection with arduino create monitor    if (volts < 2.5){    Serial.write('l');  }  else{    Serial.write('h');  }}