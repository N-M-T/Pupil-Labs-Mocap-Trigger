void setup() {  // initialize the serial communication:  Serial.begin(500000);  // initialize the ledPin as an output:  //pinMode(9, OUTPUT);}void loop() {  //read TTL voltage input  int TTLVal = analogRead(A0);  //convert analog reading to digital  float voltage = TTLVal * (5.0 / 1023.0);    //Serial.println(voltage); //for testing only    if (voltage < 3.0 ){    Serial.write('l');  }  else{    Serial.write('h');  }  }      /*  // check if data has been sent from the computer:  while (Serial.available()) {    if(Serial.read() == 'U'){      //Serial.write('U'); //code for testing speed of connection      digitalWrite(9, HIGH);      delay(10000);      digitalWrite(9, LOW);    }  }  */